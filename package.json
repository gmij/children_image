    "@babel/preset-typescript": "^7.21.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@tarojs/cli": "4.0.0",
    "@tarojs/webpack5-runner": "4.0.0",
    "@types/react": "^18.2.7",
    "babel-preset-taro": "4.0.0",
    "eslint": "^8.41.0",
    "eslint-config-taro": "4.0.0",
    "postcss": "^8.4.23",
